<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout1"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".MainActivity">

    <com.uiLibrary.bobbleUiLibrary.BobbleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"/>

    <com.uiLibrary.bobbleUiLibrary.BobbleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text"/>

    <com.uiLibrary.bobbleUiLibrary.BobbleEditText
        android:id="@+id/edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/type_your_message_here"
        android:typeface="serif" />

    <com.uiLibrary.bobbleUiLibrary.BobbleTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text1"
        android:ellipsize="end"
        android:textSize="22sp"
        android:maxLines="1"/>

    <com.uiLibrary.bobbleUiLibrary.BobbleFab
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:scaleType="centerCrop"
        app:fabSize="normal" />

    <com.uiLibrary.bobbleUiLibrary.BobbleCardView
        android:id="@+id/card2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:focusable="true" />

    <com.uiLibrary.bobbleUiLibrary.BobbleImageButton
        android:id="@+id/imgButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/add_cam" />

</LinearLayout>